{
  "name": "Julie",
  "version": "1.0",
  "description": "Tool for web navigation",
  "manifest_version": 2,
  "browser_action": {
      "default_popup": "index.html"
  },
  "background" :{
    "scripts": ["./background.js"]
  },
"web_accessible_resources": ["options.html"],
"options_page": "options.html",
"permissions" : [
    "<all_urls>",
    "storage"
  ]
} 